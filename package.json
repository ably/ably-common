    "@aws-sdk/client-s3": "^3.28.0",
    "jest": "^27.5.1",
    "jsonschema": "^1.4.0",
    "markdownlint-cli2": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.0"
